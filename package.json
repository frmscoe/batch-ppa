{
  "name": "ppa-batch-to-tms",
  "version": "0.1.0",
  "description": "ppa-batch-to-tms",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "dev": "nodemon -r dotenv/config",
    "build": "tsc --project tsconfig.json",
    "start": "node -r dotenv/config build/index.js",
    "cleanup": "rm -rf dist template jest.config.js jest.config.js.map node_modules",
    "fix": "npm run fix:prettier && npm run fix:eslint",
    "fix:eslint": "eslint --fix \"**/*.ts\"",
    "fix:prettier": "prettier --write \"**/*.ts\"",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint \"**/*.ts\"",
    "lint:prettier": "prettier --check \"**/*.ts\"",
    "test": "jest --config=jest.config.ts --passWithNoTests",
    "test:watch": "jest --config=jest.config.ts --watch",
    "prepare":"husky install"
  },
  "keywords": [
    "mojaloop",
    "actio",
    "frm"
  ],
  "contributors": [
    {
      "name": "Len Bekker"
    }
  ],
  "license": "ISC",
  "dependencies": {
    "@log4js-node/logstash-http": "^1.1.0",
    "@types/formidable": "^3.4.0",
    "arangojs": "^8.4.0",
    "axios": "^1.4.0",
    "dotenv": "^16.3.1",
    "elastic-apm-node": "^3.48.0",
    "formidable": "^3.5.0",
    "ioredis": "^5.3.2",
    "koa": "^2.14.2",
    "koa-bodyparser": "^4.4.1",
    "koa-helmet": "^7.0.2",
    "koa-router": "^12.0.0",
    "lodash": "^4.17.21",
    "log4js": "^6.9.1",
    "node-cache": "^5.1.2",
    "swagger2-koa": "^4.0.0",
    "tslib": "^2.6.1",
    "uuid": "^9.0.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/arangodb": "^3.5.17",
    "@types/axios": "^0.14.0",
    "@types/elastic-apm-node": "^2.7.0",
    "@types/eslint": "^8.44.1",
    "@types/eslint-config-prettier": "^6.11.0",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/jest": "^29.5.3",
    "@types/koa": "^2.13.8",
    "@types/koa-bodyparser": "^4.3.10",
    "@types/koa-router": "^7.4.4",
    "@types/lodash": "^4.14.196",
    "@types/log4js": "^2.3.5",
    "@types/node": "^20.4.6",
    "@types/nodemon": "^1.19.2",
    "@types/prettier": "^2.7.3",
    "@types/rimraf": "^3.0.2",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "8.0.3",
    "jest": "^29.6.2",
    "lint-staged": "^13.2.3",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0",
    "redis-mock": "^0.56.3",
    "rimraf": "^5.0.1",
    "supertest": "^6.3.3",
    "tplant": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
